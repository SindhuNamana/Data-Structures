class Solution:
    def countGoodSubstrings(self, s: str) -> int:
        i=j=0
        count = 0
        while j<len(s):
            if (j-i+1)<3:
                j+=1
            elif (j-i+1)==3:
                if len(set(s[i:j+1]))==3:
                #if s[j]!=s[j-1] and s[j]!=s[j-2] and s[j-1]!=s[j-2]:
                    count+=1
                j+=1
                i+=1
        return count

        #Time-Complexity = O(n)
        #Space-Complexity = O(1)
